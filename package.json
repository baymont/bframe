{
  "name": "bframe",
  "version": "0.1.3",
  "description": "A virtual reality framework for the web with handrails to hold on to.",
  "main": "_bundles/bframe.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "shx rm -rf _bundles lib lib-esm",
    "build": "tsc && tsc -m es6 --outDir lib-esm && webpack",
    "rebuild": "npm run clean && npm run build",
    "start:dev": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baymont/bframe.git"
  },
  "author": "Harold Martinez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baymont/bframe/issues"
  },
  "homepage": "https://github.com/baymont/bframe#readme",
  "dependencies": {
    "babylonjs": "^3.1.0-beta5"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.4.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
